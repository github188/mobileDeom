/*单选框S*/
input { padding: 0; margin: 0; border: 0; }

.Hradio:before { content: ""; display: table; }

.Hradio:after { content: ""; display: table; clear: both; }

.Hradio > div {
    position: relative;
    float: left;
    margin-right: 38px;
    width: 80px;
    height: 40px;
    line-height: 40px;
}
.Hradio > div:last-child {
    margin-right: 0;
}
.Hradio label {
    display: block;
    width: 60px;
    height: 40px;
    line-height: 40px;
    cursor: pointer;
}
.Hradio input {
    z-index: 3;
    display: block;
    opacity: 0;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 50px;
    margin: auto;
    width: 30px;
    height: 30px;
    cursor: pointer;
}
.Hradio .user-defined {
    z-index: 2;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 48px;
    margin: auto;
    width: 16px;
    height: 16px;
    border: 1px solid #ff6666;
    border-radius: 50%;
    cursor: pointer;
}
.Hradio .user-defined span.circle  {
    display: block;
    width: 12px;
    height: 12px;
    margin-top: 2px;
    margin-left: 2px;
    background-color: transparent;
    border-radius: 50%;
}
.Hradio .user-defined span.active  {
    background-color: #ff6666;
}
