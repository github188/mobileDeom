@charset "utf-8";

html,body{
    height:100%;
}
body{
    font-size: 14px;overflow: hidden;
    font-family: "microsoft yahei";
}
div {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
img {
    width: 100%;
    border: none;
}
a{
    text-decoration: none;
    color:#414141;
    font-size: 12px;
    text-align: center;
}
input::-webkit-input-placeholder {
    font-size: 14px;
    text-align: right;
    color:#999999; /*修改webkit中input的planceholder样式*/
}
input:focus::-webkit-input-placeholder {
    text-align: right;
    padding-left: 14px;
    color:#999; /*修改webkit中focus状态下input的planceholder样式*/
}

.clearfix::after {
    visibility: hidden;
    display: block;
    font-size: 0;
    content: " ";
    clear: both;
    height: 0;
}
.partnerBox {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    display: -webkit-flex;
    -webkit-flex-direction: column;
}
.partnerBox .par-top{
    width: 100%;
    margin: auto;
}
.partnerBox .par-top img {
    display: block;
}
.partnerBox .par-center {
    width: 100%;
    height: 32px;
    line-height: 32px;
    text-align: center;
    background: #F2F2F2;
}
.weui-cells,.par-center .weui-cells__title,.weui-cell{
    padding: 0;margin: 0;
}
.partnerBox .par-form{
    width: 100%;
}
.partnerBox .par-form .weui-cells{
}

.partnerBox .par-form .weui-cell {
    padding: 12px 20px;
}
.par-form .weui-cell .weui-label {
    width: 110px;
    font-size: 14px;
}
#name {
    color: #333;
    font-size:15px ;
}
.par-form .weui-cell .weui-label span{
    color: #999;
}
.par-form .weui-cell .weui-label span.marking{
    color: #EF4A46;
}
.par-form .weui-cell .weui-cell__bd .weui-cell_primary .weui-input{
    text-align: right;
}
.par-form .weui-cell .weui-flex .weui-cell__ft {
    right: 0;
}
.par-form .weui-cell img{
    width: 70px;
    height: 70px;
    border-radius: 100%;
}
.weui-cell_primary{
    text-align: right;
    font-size: 14px;
    color: #666666;
}

.partnerBox .par-bot{
    width: 100%;
    background: #F6F6F6;
    padding:20px;
    margin:auto;
    -webkit-flex: 1;
    flex: 1;
}
.partnerBox .par-bot a {
    font-size: 16px;
}
.weui-btn:after{
    border: none;
}
.bg-org {
    height: 45px;
    line-height: 45px;
    text-align: center;
    background:#ff4479;
}
