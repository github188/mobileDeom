/********** postOrderid.html*************/
input::-webkit-input-placeholder {
    font-size: 14px;
    text-align: left;
    padding-left: 10px;
    color:#DADADA; /*修改webkit中input的planceholder样式*/
}
input:focus::-webkit-input-placeholder {
    font-size: 14px;
    text-align: left;
    padding-left: 10px;
    color:rgba(255,255,255,0); /*修改webkit中focus状态下input的planceholder样式*/
}
.logs{text-align:center;margin-top:6px;}
#goback{position:fixed;top:10px;left:10px;width:30px;height:30px;text-align:center;line-height:30px;color:#fff;background:rgba(0,0,0,0.6);border-radius:50%;}
/*  kefu  */
#kefu{position:fixed;top:10px;right:10px;color:#333;}
#kefu span{display:block;width:35px;height:35px;line-height:35px;background:#fff;color:#d81e06;text-align:center;border:1px solid #d81e06;border-radius:50%;}
#kefu i.iconfont{font-size:25px;}
#kefu_mask{position:fixed;z-index:99999;background:rgba(0,0,0,0.7);width:100%;height:100%;display:none;top:0;left:0}
#kefu_mask .qrcode_box{width:65%;background:#fff;border-radius:6px;margin:30% auto 0;padding:30px 0 20px;position:relative}
#kefu_mask .qrcode_box img{display:block;width:70%;margin:auto}
#kefu_mask .qrcode_box .text{font-size:12px;color:#999;text-align:center;line-height:18px;margin-top:10px;}
#close_kefu{position:absolute;right:-10px;top:-10px;width:22px;height:22px;line-height:19px;border:1px solid #999;border-radius:50%;text-align:center;background:#fff;}
#close_kefu i.iconfont{font-size:14px;}