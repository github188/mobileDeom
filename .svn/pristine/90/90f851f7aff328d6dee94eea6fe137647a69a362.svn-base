@media screen and (max-width: 768px){
    body{
        margin: 0;   /* 1px=0.05rem  */
        padding: 0;
        background-color: #ffffff;
    }
    .cont_container{
        display: block;
        width: 100%;
    }
     .cont_container .faq_cont_container{
         width: 100%;
         display:-moz-box;
         -moz-box-pack:center;
         -moz-box-align:center;
    }
     /* 菜单导航栏s*/
    .cont_container .faq_cont_container ul, li{
        padding:0;
        margin:0;
        /*margin-left: 3%;*/
        z-index: 600;
    }
    .cont_container .faq_cont_container ul li{
        float:left;
        margin-right:1px;
        display:inline;
        list-style:none;
        text-align:center;
        width: 100%;
    }
     /* 遮罩层样式*/
    .cont_container .faq_cont_container ul .mainLi .bgc_mask{
        background: rgba(0,0,0,0.5);
        /* z-index: 999; */
        width: 100%;
        height: 100%;
        /*position: fixed;*/
        /*top: 354px;*/
        /*left: 0;*/
    }
    /*箭头样式*/
     .arrows-down{
        width: 10px;
        height: 10px;
        border-top: 2px solid #333333;
        border-left: 2px solid #333333;
        margin-left: 10px;
        display: inline-block;
        transform: rotate(-135deg);
        -webkit-transform: rotate(-135deg);
        -moz-transform: rotate(-135deg);
        -ms-transform: rotate(-135deg);

    }
    .arrows-top{
        width: 10px;
        height: 10px;
        border-top: 2px solid #333333;
        border-left: 2px solid #333333;
        margin-left: 10px;
        display: inline-block;
        transform: rotate(-315deg);
        -webkit-transform: rotate(-315deg);
        -moz-transform: rotate(-315deg);
        -ms-transform: rotate(-315deg);
        vertical-align: -webkit-baseline-middle;
    }
    .arrows-right{
        width: 10px;
        height: 10px;
        border-top: 2px solid #00ace9;
        border-left: 2px solid #00ace9;
        margin-left: 10px;
        display: inline-block;
        transform: rotate(-225deg);
        -webkit-transform: rotate(-225deg);
        -moz-transform: rotate(-225deg);
        -ms-transform: rotate(-225deg);
        vertical-align: middle;
        color: #e5e5e5;
    }
    .jqArrows-right{
        width: 10px;
        height: 10px;
        border-top: 2px solid #00ace9;
        border-left: 2px solid #00ace9;
        margin-left: 10px;
        display: inline-block;
        transform: rotate(-225deg);
        -webkit-transform: rotate(-225deg);
        -moz-transform: rotate(-225deg);
        -ms-transform: rotate(-225deg);
        vertical-align: middle;
        color: #e5e5e5;
    }

    .cont_container .faq_cont_container ul li ul li{
        float: none;
        background: #ccc;
        display: block;
        _display: inline;
        position: relative;
        width: 190%;
        border-bottom: 1px solid #e5e5e5;
    }
    .cont_container .faq_cont_container ul li ul{
        display:none;
        position: absolute;
    }
    .cont_container .faq_cont_container ul a{
        text-decoration: none;
        height: 50px;
        line-height: 50px;
        display: block;
        background: #fff;
        color: #666666;
        font-size: 12px;
    }
    .cont_container .faq_cont_container ul li a:hover{
        background:#eee;
        color:#00ace9;
    }
    /*二级*/
    .cont_container .faq_cont_container ul li ul li .aaa{
        position:absolute;
        left: 101%;
        top:0;
        border-left:1px solid #fff;
    }
    .cont_container .faq_cont_container ul li ul li .aaa li{
        float: none;
        width: 1340%;
        text-align: center;
    }
    /*三级*/
    .cont_container .faq_cont_container ul li ul li .aaa li .aaa{
        position:absolute;
        left: 118px;
        top:0;
        border-left:1px solid #fff;
    }
    body .bbb{
        background:#f00;
        color:#fff
    }
    /* 菜单导航栏e*/
    .cont_container .faq_cont_container .searchBox {
        min-width: 200px;
        position: relative;
        padding: 1.2rem 2.2rem;
        box-sizing: border-box;
        border-bottom: 1px solid #f5f5f5;
    }
    .cont_container .faq_cont_container .searchBox .kf_mod_searchBox {
        /*width:310px;   /!*  �����ȶ���*!/*/
    }
    .kf_mod_searchBox .searchBar {
        width: 100%;
        height: 40px;
        position: relative;
        background-color: #fff;
    }
    .kf_mod_searchBox .searchBar input {
        border: 1px solid #dedede;
        background-color: transparent;
        height: 38px;
        line-height: 38px;
        width: 87%;
        padding-left: 2%;
        padding-right: 10%;
        font-size: 14px;
        color: #666;
        -webkit-transition: border-color .3s;
        transition: border-color .3s;
        position: absolute;
        left: 0;
        top: 0;
        border-radius: 4px;
    }
    .kf_mod_searchBox .searchBar button {
        width: 45px;
        height: 40px;
        position: absolute;
        right: 0;
        top: 0;
        display: inline-block;
        border: none;
        cursor: pointer;
        background-color: #00ace9;
        color: #fff;
    }
    .kf_mod_searchBox .searchBar button:before{
        content: "";
        display: inline-block;
        width: 20px;
        height: 20px;
        z-index: 600;
        background-image: url("../image/icon-soushuo.png");
        background-repeat: no-repeat;
        margin-top: 5px;
        background-position: center center;
    }
    .cont_container .faq_cont_container .faq_cont_list {
        padding: 0.9rem 1.25rem;
        overflow: hidden;
    }
    .cont_container .faq_cont_container .content{
        overflow: auto;
    }
    .cont_container .faq_cont_container .faq_cont_list .tip {
        border-left: 4px solid #00ace9;
        padding-left: 7px;
        color: #2d2d2d;
        line-height: 1;
        margin: 12px 0;
        margin-bottom: 20px;
    }
    .cont_container .faq_cont_container .faq_cont_list .questionList {
        margin-bottom: 1.7rem;
        margin-top: 0.5rem;
    }
    .cont_container .faq_cont_container .faq_cont_list .questionList dl dd {
        margin-bottom: 0.25rem;
    }
    .cont_container .faq_cont_container .faq_cont_list .questionList dl dd a {
        font-size: 15px;
        line-height: 30px;
        color: #333;
    }
    .cont_container .sw-modBox{
        background: #fff;
        font-size: 14px;
        padding: 3%;
    }
    .cont_container .sw-modBox .sw_modBox_border{
        border: 1px solid #e8eef6;
        padding: 14px 10px;
    }
    .cont_container .sw-modBox .sw_modBox_border p{
        margin: 5px 0;
        line-height: 28px;
    }

}