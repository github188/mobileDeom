    -webkit-flex: 3;
    flex: 3;
}
.copy{width: 40%;height: 28px;background:#ff477b ;text-align: center;line-height: 28px;color: #FFFFFF;
    position: relative;left: 30%;top: 14px;
}
.amoyPwd{ width: 86%;height: 60px;line-height: 65px;text-align: center;
    margin: auto;border:2px dashed  #ff477b;border-radius: 5px;font-size: 16px;}
.amoy-m-c{
    position:relative ;
}
.amoy-m-c a{
    width: 50%;
    height: 45px;
    text-align: center;
    border-radius: 50px;
    color: #FFFFFF;
    display: block;
    font-size: 15px;
    margin:20px auto;
}
.amoy-m-b {position: relative;margin-bottom: 10px;}
.amoy-m-b .p-c0b8fd2{
    font-size:14px ;
    text-align: center;
    color: #0b8fd2;
}
#oneKeyCopy{
    line-height: 45px;
    background: #ff477b;
}
#copySuccess{
    line-height: 45px;
    display: none;
    background: #66cc00;
    position: absolute;
    top: -20px;
    left: 25%;
}
.amoy-bot{
    position: relative;
    border-radius:  0 0 5px 5px;
    background: #F0F0F0;
    -webkit-flex: 2;
    flex: 2;
}
.amoy-bot .p-c7e4b40{
    color: #7e4b40;
    font-size: 14px;
    padding:10px 25px;
}
.tips{
    width: 80%;
    left:10%;border-radius:20px;background:rgba(0,0,0,0.8);padding:8px 0;
    position: absolute;
