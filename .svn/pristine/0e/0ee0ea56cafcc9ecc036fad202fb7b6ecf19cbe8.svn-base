@charset "utf-8";
*{margin: 0 ;padding: 0;}
li{list-style: none;}
html,body{
    height:100%;
}
body{
    font-size: 14px;
    font-family: "microsoft yahei";
}
div {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
img {
    width: 100%;
    border: none;
}

a{
    border: none;
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    text-decoration: none;
    color:#414141;
    font-size: 12px;
    text-align: center;
}
.clearfix::after {
    visibility: hidden;
    display: block;
    font-size: 0;
    content: " ";
    clear: both;
    height: 0;
}
.productBox{width: 100%;height: 100%; background: #f6f6f6;}
.weui-updown {width: 100%; margin: auto;}
.pro-pic {width: 100%;}
.pro-pic img{display: block;}
.pro-title {padding:10px 20px;text-align: left;font-size: 14px;color: #333;font-weight: normal;
    border-bottom: 1px solid #EAEAEA;background: #FFFFFF;}
.pro-price{padding:0 20px; height:65px;display: flex;margin-bottom: 5px;background: #FFFFFF;}
.pro-price .pro-price-l{
    flex: 2;
    margin: auto;
}
.pro-price .pro-price-1{font-size:12px;color: #ff3333;}
.pro-price .pro-price-1 span {font-size: 20px;font-weight: bold;}
.pro-price .pro-price-1 small {font-size: 12px;font-weight: normal;}
.pro-price .pro-price-2{color: #999;font-size: 12px;text-decoration: line-through;}
.pro-price .pro-price-r{
    text-align: right;
    flex: 3;
    line-height:65px ;
}
.pro-price .pro-price-r .pro-price-r-1{color: #999;font-size: 14px;}
.pro-price .pro-price-r .pro-price-r-1 label{
    display: inline-block;
    width: 50%;
    text-align: center;
    color: #FFFFFF;
    background:url(../../public/images/youhuiBg.png) no-repeat center center ;
    background-size:100% ;
}
.pro-price .pro-price-r .pro-price-r-1 label span{font-size: 24px;font-weight: bold;
    display: inline-block;vertical-align: middle;}
.pro-price .pro-price-r .pro-price-r-1 label span:nth-child(1){
    font-size: 14px;
    font-weight: normal;
    margin-top: 5px;
}

.pro-groom {
    background: #FFFFFF;
    padding: 10px 20px;
    font-size: 12px;
    color: #999;
}
.pro-groom .pro-groom-desc span{
    border: 1px solid #ff477b;
    padding: 0 5px;
    border-radius: 5px;
    color: #ff477b;
    text-align: center;
}
.pro-groom .pro-groom-desc p{
    display: inline-block;
    margin-left: 3px;
}
.weui_tabbar {
    position: fixed;
    bottom: 0;
}
.font146 {
    font-size: 14px;color: #666666;
}
.pro-detail-pic img{width: 100%; display: block;}

.pro-remove {
    background: #F6F6F6;width: 100%;height:70px;text-align: center;margin: auto;line-height: 70px;
}
/*上拉下拉*/
.dropload-up,
.dropload-down{
    position:relative;height:0;overflow:hidden;font-size:12px;
    -webkit-transform:translateZ(0);transform:translateZ(0)}
.dropload-down{height:50px}
.dropload-refresh,
.dropload-update,
.dropload-load,
.dropload-noData{height:50px;line-height:50px;text-align:center}
.weui-loading {
    width: 20px;
    height: 20px;
    display: inline-block;
    vertical-align: middle;
    -webkit-animation: weuiLoading 1s steps(12, end) infinite;
    animation: weuiLoading 1s steps(12, end) infinite;
    background: transparent url(data:image/svg+xml;base64,) no-repeat;
    background-size: 100%;
}
@-webkit-keyframes weuiLoading {
    0% {
        -webkit-transform: rotate3d(0, 0, 1, 0deg);
    }
    100% {
        -webkit-transform: rotate3d(0, 0, 1, 360deg);
    }
}
@keyframes weuiLoading {
    0% {
        -webkit-transform: rotate3d(0, 0, 1, 0deg);
    }
    100% {
        -webkit-transform: rotate3d(0, 0, 1, 360deg);
    }
}
/****************footer*************************/
.page-foot{
    width:100%;
    height: 48px;
    position: fixed;
    bottom: 0;
    left: 0;
}

.page-foot .g-foot-bar{
    width: 100%;
    height: 100%;
    background-color: #fff;
    z-index: 1000;
}
ul.m-nav-foot{
    width: 100%;
    height: 100%;
}
ul.m-nav-foot li{
    float:left;
    height: 100%;
    margin: auto;
    text-align: center;
}
ul.m-nav-foot li:nth-child(1),ul.m-nav-foot li:nth-child(2){
    border-top: 1px solid #D6D6D6;
    width: 17.5%;
    box-sizing: border-box;
}
ul.m-nav-foot li:nth-child(3),ul.m-nav-foot li:nth-child(4){
    width: 32.5%;
}
/*ul.m-nav-foot li:nth-child(1){*/
    /*border-right:1px solid #E1E1E1 ;*/
/*}*/
.m-nav-foot li a{
    display: block;
    width: 100%;
    height: 100%;
}
.m-nav-foot li.cur a{
    color: #ff4479;
}
.m-nav-foot li a p{ line-height: 1.3;}
.m-nav-foot li a i{
    font-size: 23px;
}

.m-nav-foot a div {
    width:20px;
    height: 20px;
    margin:5px auto;
}
.m-nav-foot a.bot-coupons1,.m-nav-foot a.bot-coupons2{
    line-height:50px;
    font-size: 16px;
    text-align: center;
    color: #FFFFFF;
}
.m-nav-foot a.bot-coupons1{
    background: -webkit-linear-gradient(left, #ff6633, #ff9900); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right,  #ff6633, #ff9900); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right, #ff6633, #ff9900); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right,  #ff6633, #ff9900); /* 标准的语法 */
}
.m-nav-foot a.bot-coupons2{
    background: -webkit-linear-gradient(left,#f5618b,#ff4479); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right,#f5618b,#ff4479); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right, #f5618b,#ff4479); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right, #f5618b,#ff4479); /* 标准的语法 */
}

/*****************淘口令领券********************************/
.amoyPwdBox{width: 100%;height: 100%;background: rgba(0,0,0,0.5);z-index: 99999;position: absolute;top: 0;}
.amoyBox{width: 86%;
    position: absolute;top: 12%;left: 7%;
    display: -webkit-flex;-webkit-flex-direction: column;
    display: flex;flex-direction: column;
    border-radius: 5px;
}
.amoy-top{
