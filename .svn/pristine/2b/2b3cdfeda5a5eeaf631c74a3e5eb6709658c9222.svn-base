         textarea{
           width: 6.6rem;
           min-height: 1.0rem;
           border: 1px solid #e5e5e5;
           border-radius: 4px;
           font-size: 14px;
           color: #808080;
            padding: 0.16rem 0 0 0.2rem;
         }
         .bot{
           text-align: right;
           margin-top: .2rem;
           position: relative;
           a{
             width: 0.8rem;
             .eleHeight( 0.5rem);
             background-color: #ff6100;
             text-align: center;
             border-radius: 2px;
             display: inline-block;
             margin-left: 0.2rem;
             color: #ffffff;
           }
         }
       }
       /* 文本框E */
       .noteFoot{
         .eleHeight( 0.7rem);
         font-size: 13px;
         .operate{
           span{
             display: inline-block;
             font-size: 16px;
             i.iconfont{
                font-size: 18px;
                vertical-align: initial;
                margin-right: 0.06rem;
             }
             i.icon-bofang{
               color: #76bdff;
               font-size: 22px;
               vertical-align: middle;
             }
             em{
               display: inline-block;
               border: 1px solid #e5e5e5;
               border-left: none;
               font-size: 12px;
               .eleHeight(0.38rem);
               background-color: #f7f7f7;
               color: #666666;
               margin-left: -0.12rem;
             }
           }
         }
       }
     }
     /* 遮罩层S */
     .Hmask{
       width: 100%;
       height: 100%;
       background-color: rgba(0,0,0,0.5);
       position: fixed;
       top: 2.8rem;
       left: 0;
       z-index: 666;
       display:none;
       .maskDel{
         width: 40%;
         margin: 0 auto;
         background-color: #fff;
         padding: 0.2rem;
         margin-top: 2.6rem;
         text-align: center;
         .deleWZ{
           .eleHeight(0.9rem);
           font-size: 18px;
           text-align: center;
         }
         .maskFrom{
           margin-bottom: 0.3rem;
           margin-top: 0.2rem;
           a{
             width: 1.28rem;
             .eleHeight(0.6rem);
             display: inline-block;
             color: #ffffff;
             &.Hcancel{
               background-color: #808080;
               margin-right: 0.16rem;
             }
             &.Hsubmit{
               background-color: #f00;
             }
           }
         }
       }
     }
     /* 遮罩层E */
     /* 动画S */
     .delBoxAni{
       animation: bounceInDown 1s;
       -webkit-animation: bounceInDown 1s;
     }
     @keyframes bounceInDown  {
       0%{
          opacity: 0;
          .transformY(-2000px);
       }
       50%{
          opacity: 1;
          .transformY(100px);
       }
       80%{
          opacity: 1;
          .transformY(-50px);
       }
       90%{
          opacity: 1;
          .transformY(10px);
       }
       100%{
          opacity: 1;
          .transformY(0px);
       }
     }
     @-webkit-keyframes bounceInDown {
       0%{
          opacity: 0;
          .transformY(-2000px);
       }
       50%{
          opacity: 1;
          .transformY(100px);
       }
       80%{
         opacity: 1;
          .transformY(-50px);
       }
       90%{
         opacity: 1;
          .transformY(10px);
       }
       100%{
           opacity: 1;
          .transformY(0px);
       }
     }
     /* 动画S */
   }

