.transform (@angeles){
  -ms-transform: rotate(@angeles);
  -webkit-transform: rotate(@angeles);
  -o-transform: rotate(@angeles);
  -moz-transform: rotate(@angeles);
}
.eleHeight (@rem){
  height: @rem;
  line-height: @rem;
}
.widHig(@remw,@remH){
   width: @remw;
   height: @remH;
}
.transformY(@px){
   transform: translateY(@px);
   -webkit-transform: translateY(@px);
}
.Rtransform(@deg){
   -ms-transform: rotate(@deg);
   -webkit-transform: rotate(@deg);
   -o-transform: rotate(@deg);
   -moz-transform: rotate(@deg);
}

.background(@url,@boolean:true){
   background-image: url('../@{url}');
   & when(@boolean){
       background-repeat: no-repeat;
   }
}
/* listenCourse.html */
.headCom{
   position: relative;
   .topmeun{
      width: 0.48rem;
      height: 1.5rem;
      display: inline-block;
      position: absolute;
      left: 0;
      top: 0;
      background-color: #ffffff;
      z-index: 800;
      .icon-icon-back-left{
         display: inline-block;
         position: absolute;
         top: 50%;
         margin-top: -0.28rem;
         color: #666666;
         font-size: 28px;
      }
   }
   .com{
      background: #ffffff;
      height:1.5rem;
      width: 100%;
      //border-bottom: 2px solid #e5e5e5;
      ul{
         height: 100%;
