/*~~common.css~~*/
/*清楚浮动*/
.clean_float{clear: both;}
/*.toolBar*/
.toolBar{ width:100%; height: 34px; line-height: 34px; background-color: #545454; }
.toolBar .in{ height: 100%; color: #ccc; position: relative; margin: 0 auto; text-align:left;}
.toolBar a{ color: #ccc; display: inline-block;}
.toolBar a:hover{ color: #ff6100; }
.toolBar .inL .sp{ width: 110px;}
.toolBar .sp{ display: inline-block;float: left; }
.toolBar .sp:after{ content: ""; width: 1px; height: 14px; background-color: #ccc; vertical-align: middle; display: inline-block; margin:0 14px;}
.toolBar .sp.wx{ margin-left: -4px;_margin-left: -4px; position: relative;}
.toolBar .sp.wx a:hover{ color: #bfbfbf;}
.toolBar .sp.wx a:hover i{ color: #4cbf2a;}
.toolBar .sp.wx:after,.toolBar .sp.kf:after{ display: none;}
.toolBar .sp i{  font-size: 32px; vertical-align: middle;}
.toolBar .sp span{ margin-left: -8px;}
.ewmPop{ width: 176px; height: 206px; position: absolute; left: 0; top:35px; z-index: 5; display: none;}
.ewmPop i{ position: absolute;  left:20px;  top:0;  z-index: 1;  width: 14px; height: 10px; background: url("../images/itopJt.png") no-repeat;}
.ewmPop .con{ width: 144px; padding: 15px; background-color: #fff; border: 1px solid #e0e0e0; text-align: center; position: absolute; left: 0; top: 8px; }
.ewmPop .con a{ width: 132px; height: 132px; overflow: hidden; display: inline-block;}
.ewmPop .con a img{ width: 100%; height: 100%;}
.ewmPop .con p{ font-size: 14px; color: #ccc; line-height: 20px;}

/*.header*/
.header{ width:100%; height: 90px; background-color: #fff;}
.headerDown {  position: fixed;  z-index: 99999999; top: 0;  box-shadow: 1px 0 5px #ebe9e9; }
.header .in{ height: 100%; position: relative; }
.header .inL{ width: 290px;height: 100%; position: absolute; }
.header .logo{ width: 200px;  height:90px;  background:url("../images/logo.png") no-repeat; background-size: contain;  display: block; }
.header .logo h1{ font-size: 0;}
.header .logo a{ width: 200px; height: 0; padding-top: 90px; overflow: hidden; display: block; }
.header .inR{ width: 810px;height: 100%; position: relative; }
.header .nav{ height: 100%; }
.header .nav ul{ height: 100%;}
.header .nav li{ height: 100%; line-height:90px; float:left;position: relative;width: 73px;margin: 0 15px; text-align: center;}
/*新增 Hlist */
.header .nav li .Hlist{
    display: none;position: absolute;top: 88px;left: -20px;width: 140px;/*height: 210px;*/background-color: #ffffff;z-index: 100; }
.header .nav li .Hlist ul li{
    width: 80%; height: 42px; line-height:42px; border-bottom: 1px solid #e5e5e5; text-align: center; }
.header .nav li .Hlist ul li a{ color: #666;}
.header .nav li .Hlist ul li a:hover{ color: #ff6100;}
.header .nav li:last-of-type{ margin-right: 0;}
.header .nav li.mes{ position: relative; margin-right: 0;}
.header .nav li.mes .mesNum{ width: 20px; height: 20px; line-height: 20px; background-color:#ff4242; text-align: center; border-radius: 50%; font-size: 12px; color: #fff; font-style: normal;
    position: absolute; right:4px; top: 24px; cursor: pointer;}
.header .nav li a{ display: block; font-size: 16px;}
.header .nav li span{  display: block;  position: absolute;  width: 0;  height: 0;  background: #ff6100;  bottom: 2px;  left:50%; right: auto; transition: all .3s ease;
    -webkit-transition: all .3s ease;-moz-transition: all .3s ease;-ms-transition: all .3s ease;}
.header .nav li.cur a.ti,.header .nav li a.ti:hover{ color: #ff6100;}
.header .nav li.cur span,.header .nav li:hover span{ width: 100%; height: 2px; left: 0; right: 0;}
.header .rCon{ margin-top: 25px; }
.header .rCon .sel{ width: 190px; height: 36px; line-height: 36px; border:1px solid #f5f5f5; background-color: #f5f5f5; border-radius: 20px; overflow: hidden; position: relative;}
.header .rCon .sel input{  width: 140px; height: 36px; line-height: 36px; background-color: #f5f5f5; padding-left: 15px;font-size: 14px;}
.header .rCon .sel.cur{ background-color: #fff; }
.header .rCon .sel.cur input{ background-color: #fff;}
.header .rCon .sel .ico{ position: absolute; right:3px; top: -1px; font-size: 30px; color: #ccc; cursor: pointer;}
.header .rCon .myTx{ position: relative;}
.header .rCon .myTx .tx{ width: 40px; height: 40px; border-radius: 50%; overflow: hidden; border: 1px solid #e0e0e0; margin-left: 35px;}
.header .rCon .myTx .tx a{ display: block;}
.header .rCon .myTx .tx img{ width: 100%;}
.header .myList{ width: 172px; height: 229px; position: absolute; top: 43px; right:3px; background-color: #fff; z-index: 5; display: none;}
.header .myList i{ position: absolute;  right:10px;  top:0;  z-index: 1;  width: 14px; height: 10px; background: url("../images/itopJt.png") no-repeat;}
.header .myList ul{ border: 1px solid #e0e0e0; border-bottom: none; background-color: #fff; width: 170px; position: absolute; top: 8px; left: 0;}
.header .myList li{ width: 100%; height: 54px; line-height:54px; border-bottom: 1px solid #e0e0e0; text-align: center;}
.header .myList li a{ display: block; font-size: 16px; color: #333;}
.header .myList li a:hover{ color: #ff6100;}
.header .rCon .loginBtn{ margin-left: 15px;}
.header .rCon .loginBtn a{ width: 68px; height: 38px; line-height: 38px; background-color: #ff6100; border-radius: 20px; color: #fff; font-size: 14px;  display: inline-block; text-align: center; }

/*.footer*/
.footer{ width:100%; height: auto; background-color: #545454; color: #999; }
.footer .footHd{ height: 170px; padding-top: 40px;}
.footer .hdL{ width: 324px;}
.footer .telTime .tel{ margin-top: 15px; color: #fff;}
.footer .telTime .tel .p1{ font-size: 16px; }
.footer .telTime .tel .p2{ font-size: 26px; margin-top: 10px;}
.footer .telTime .time{ font-size: 14px; margin-top: 5px;}
.footer .hdM{ width: 418px;}
.footer .hdM .item{ width: 33.33%; float: left;}
.footer .hdM .item h6{ color: #fff; font-size: 14px;}
.footer .hdM .item ol{ overflow: hidden; margin-top: 17px;}
.footer .hdM .item li{ margin-top: 10px;}
.footer .hdM .item li a{ color: #999; font-size: 14px;}
.footer .hdM .item li a:hover{ color: #fff;}
.footer .hdR{ width: 348px; padding-left: 10px;}
.footer .hdR .ewm{ width: 120px; text-align: center; margin-left: 50px; float: left;}
.footer .hdR .ewm p{ color: #fff; font-size: 14px;}
.footer .hdR .ewm a{ width: 120px; height: 120px; overflow: hidden; display: block; margin-top: 5px;}
.footer .hdR .ewm a img{ width: 100%;}
.footer .links{ border-top: 1px solid #666; padding-top: 20px; padding-bottom: 5px;}
.footer .links .tit h5{ color: #fff; font-size: 14px;}
.footer .links ul{ margin-left: 130px; overflow: hidden;}
.footer .links li{ float: left; margin-bottom: 10px; margin-right: 30px;}
.footer .links li a{ color: #999; font-size: 14px;}
.footer .links li a:hover{ color: #fff;}
.footer .copyright{border-top: 1px solid #666; padding-top: 15px; padding-bottom: 35px;text-align: center; }
.footer .copyright p{ font-size: 12px; }
.footer .copyright p a{ color: #999;}
.footer .copyright p a:hover{ color: #fff;}
.footer .copyright ul{ overflow: hidden; margin-top: 35px; text-align: center; display: inline-block;}
.footer .copyright ul li{ float: left; margin: 0 8px;}

/*sidebar*/
.sidebar{ position: fixed; left: auto; bottom: auto; right: 50px; top: 50%; margin-top: -50px; z-index: 99;}
.sidebar .in{ width: 52px; background-color: #fff; border: 1px solid #e6e6e6; border-bottom: none;}
.sidebar li{ border-bottom: 1px solid #e6e6e6;/* width: 100%;*/ height: 52px; position: relative; transition: all 0.3s ease; -webkit-transition: all 0.3s ease;-moz-transition: all 0.3s ease;-ms-transition: all 0.3s ease; }
.sidebar li a{ width: 52px; height: 52px; background: url("../images/rightNav.png") no-repeat;  display: inline-block;}
.sidebar li.li01 a{ background-position: 0 0;}
.sidebar li.li02 a{ background-position: 0 -52px;}
.sidebar li.li03 a{ background-position: 0 -104px;}
.sidebar li.li04 a{ background-position: 0 -156px;}
.sidebar li.li05 a{ background-position: 0 bottom;}
.sidebar li.li01.hover a{ background-position: right 0;}
.sidebar li.li02.hover a{ background-position: -52px -52px;}
.sidebar li.li03.hover a{ background-position: -52px -104px;}
.sidebar li.li04.hover a{ background-position: -52px -156px;}
.sidebar li.li05.hover a{ background-position: -52px bottom;}
.sidebar li.li01 i{ width: 20px; height: 20px; line-height: 20px; text-align: center; background-color: #ff4242; color: #fff; font-size: 12px; font-style: normal; border-radius: 50%;
    position: absolute; right: -2px; top: -5px; z-index: 1;}
.sidebar .lePop{ width: 92px; height: 96px; padding: 8px; background-color: #fff; border: 1px solid #e6e6e6; text-align: center; position: absolute; top: -28px; right:100px; z-index: 88;
    opacity: 0;  filter: alpha(opacity=0); display: none;  transition: all 0.3s ease; -webkit-transition: all 0.3s ease;-moz-transition: all 0.3s ease;-ms-transition: all 0.3s ease;}
.sidebar .lePop img{ width: 80px; height: 80px; display: inline-block;}
.sidebar .lePop span{ display: inline-block; margin-top: 2px;}
.sidebar .lePop .arr{ width: 7px; height: 14px; background: url("../images/jtrr.png") no-repeat;  position: absolute; right: -7px; top: 45px;  z-index: 22; }
.sidebar li.hover{ background-color: #76bdff;}
.sidebar li.hover .lePop{ display:block; }

/*公告文字滚动*/
.gfNot{ width: 760px; height: 52px;padding: 0 15px; background-color: #fff; overflow: hidden; font-size: 14px; }
.gfNot .tit{ line-height: 52px; width: 115px;}
.gfNot .tit h6{ display: inline-block; margin-left: 5px; font-size: 14px;}
.gfNot .tit i{ width: 20px; height: 20px; background: url("../images/icoNot.png") no-repeat; display: inline-block; vertical-align: middle;}
.gfNot ul{ margin-left: 130px;}
.gfNot li{ height: 52px; line-height: 52px;}
.gfNot li a{ font-size: 14px;}
.gfNot li a:hover{ color: #ff6100;}
.gfNot li p{ float: left; width: 85%;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.gfNot li span{ float: right;}

/*视频课程列表*/
.courLists{ margin-top: 30px; padding-top:20px; overflow: hidden; padding-left: 5px; }
.courLists li{ float: left; max-width: 240px; min-height: 240px; height: 240px; margin: 0 0 28px 14px; position: relative;}
.courLists li .item{ width: 226px; height: 226px;  padding: 6px; overflow: hidden; border: 1px solid transparent; transition: all .5s ease;
    -webkit-transition: all .5s ease; -moz-transition: all .5s ease;-ms-transition: all .5s ease; }
.courLists .item .tp{ width: 100%; height: 142px; overflow: hidden; border-radius: 4px; position: relative;}
.courLists .item .tp i{ font-size: 20px; color: #ff6e6e; position: absolute; right: 8px; top:8px; z-index: 1; cursor: pointer;}
.courLists .item .tp .lab{ width: 62px; height: 62px; background-repeat: no-repeat; position: absolute; left: 0; top: 0; z-index: 1;}
.courLists .item .tp .lab.labNew{ background-image: url("../images/lab01.png");}
.courLists .item .tp .lab.labTj{ background-image: url("../images/lab02.png");}
.courLists .item .tp img{ width: 100%; height: 100%;}
.courLists .item .txt{ margin-top:10px;}
.courLists .item .txt a{ font-size: 14px;}
.courLists .item .tiName{ font-size: 14px; line-height: 20px;}
.courLists .item .bot{ margin-top: 10px;color: #999; overflow: hidden; line-height: 24px; width: 226px; position: absolute; bottom: 6px; left: 6px;}
.courLists .item .bot a{ color: #999;}
.courLists .bot .money em{ color: #ff6e6e; font-size: 14px; margin-right: 1px;}
.courLists .bot .delNo{text-decoration: line-through; color: #999;}
.courLists .bot .delNo em{ color: #999;}
.courLists .item .handle i{ color: #76bdff; font-size: 20px;vertical-align: sub;}
.courLists li .item{ transition: transform .4s ease;-webkit-transition: transform .4s ease;-moz-transition: transform .4s ease;-ms-transition: transform .4s ease;}
.courLists li:hover .item { /*transform: scale(1.05); -webkit-transform: scale(1.05); -moz-transform: scale(1.05); -ms-transform: scale(1.05);*/}
.courLists li:hover .item,.courList li.hover.item{ background-color: #fff; border-radius: 4px;/* border-color: #e6e6e6;  box-shadow: 0 0 12px #e1e1e1;*/}
.courLists li .item img{ transition: transform .4s ease;-webkit-transition: transform .4s ease;-moz-transition: transform .4s ease;-ms-transition: transform .4s ease; }
.courLists li:hover .item img,.courList li.hover.item img{transform: scale(1.1); -webkit-transform: scale(1.1); -moz-transform: scale(1.1); -ms-transform: scale(1.1);}
.courLists li .tiName a:hover{ color: #76bdff;}
.load{ text-align: center; height:50px;line-height:50px; }
.load .loadBtn{ width:90%; height: 40px; line-height: 40px; margin: 0 auto; text-align: center; color:#999; background-color: transparent; border-radius: 6px;font-size: 14px;
    display: block;  transition: all .6s ease;-webkit-transition: all .6s ease;-moz-transition: all .6s ease;-ms-transition: all .6s ease;}
.load .loadBtn:hover{ color: #ff6100;}
.load .loadBtn i{ width: 16px; height: 16px; background: url("../images/loadingIco01.png") no-repeat; display: inline-block; vertical-align: middle; margin-right: 5px;}
.load .loadBtn.loading i{ background-image: url("../images/loadingIco.gif");}

/*提示*/
.tip,.tip2, .tip3{ color: #ff1717; display: inline-block; margin-left: 5px; line-height: 40px; }
/*翻页*/
.pages{
    display: inline-table;
    height: 50px;
    text-align: center;
    padding-top: 20px;
}
.pages a,.pages span{
    display: inline;
    border-radius: 4px;
    margin: 0 3px;
    background-color: #fff;
    border: 1px solid #ddd;
    color: #ff6100;
    float: left;
    line-height: 1.42857;
    padding: 6px 10px;
    position: relative;
    text-decoration: none;
}
.pages a:hover{
    background-color: #eee;
    border-color: #ddd;
    color: #111;
    z-index: 2;
}
.pages .current{
    background-color: #ff6100;
    border-color: #ff6100;
    color: #fff;
    cursor: default;
    z-index: 3;
}
.pages >div{
    line-height: 32px;
    margin-left: 250px;
}
.pages .prev_disabled , .pages .next_disabled{
    cursor: no-drop;
}

/*IE提示*/
.chromeframe { text-align: center;  font-size: 20px;  line-height:60px; height: 60px; background-color: #2d2d2d; color: #fff;
    verflow: hidden;white-space: nowrap;text-overflow: ellipsis; }
.chromeframe a {  font-size: 20px;  color: #56a1ff; text-decoration: underline; }

/*stuElite.html*/
.stuElite{ padding: 20px 0 10px 15px;overflow: hidden; }
.stuEliste2{ padding-top: 0; padding-left: 0; padding-bottom: 0;}
.stuEliste2 .elList li{ margin-left: 7px; margin-right: 8px;}
.elList li{ width: 260px; height:400px; float: left; background-color: #fff; margin:0 10px 10px; position: relative;
    transition: all ease .2s; -webkit-transition: all ease .2s; -moz-transition: all ease .2s; -ms-transition: all ease .2s;
}
.elList li .item{  }
.elList .tp{ width: 258px; height: 238px; overflow: hidden; border: 1px solid #ebebeb;}
.elList .tp img{ width: 100%; height: 100%;}
.elList .txt{ width: 258px; height: 69px; background-color: #fff;border: 1px solid #ebebeb; border-top:none;padding: 10px 0; overflow: hidden;
    position: absolute; left: 0; top: 240px;z-index: 10;
    transition: all ease .2s; -webkit-transition: all ease .2s; -moz-transition: all ease .2s; -ms-transition: all ease .2s;
}
.elList .tiName{ font-size: 16px; text-align: center;}
.elList .wz{ color: #999; font-size: 14px;  padding: 0 20px; margin-top: 5px;}
.elList li.hover{ box-shadow: 0 0 10px #ebebeb;-webkit-box-shadow: 0 0 10px #ebebeb;-moz-box-shadow: 0 0 10px #ebebeb;  }
.elList li.hover .txt{ height: 140px;}


/*other.css*/
.js-ck{ width: 16px; height: 16px; vertical-align: sub;}